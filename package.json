{
  "name": "statical",
  "version": "1.0.0",
  "description": "Static site generator boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "node ./bin/dev.js"
  },
  "author": "Kevin Lint",
  "license": "ISC",
  "dependencies": {
    "async": "^1.2.1",
    "autoprefixer": "^5.2.0",
    "browser-sync": "^2.7.6",
    "cssnext": "^1.6.0",
    "csswring": "^3.0.5",
    "del": "^1.2.0",
    "front-matter": "^1.0.0",
    "fs": "0.0.2",
    "node-sass": "^3.1.2",
    "postcss": "^4.1.11",
    "swig": "^1.4.2",
    "uglify-js": "^2.4.23"
  },
  "build": {
    "patterns": {
      "scripts": [],
      "styles": [
        "settings",
        "grid",
        "typography"
      ]
    },
    "property": {
      "name": "kevinlint.com",
      "scripts": [
        "main"
      ],
      "styles": [
        "main"
      ]
    },
    "pages": [
      "index",
      "about"
    ],
    "parts": [
      "menu",
      "head",
      "footer-scripts",
      "header"
    ],
    "pieces": [
      "logo"
    ]
  }
}
