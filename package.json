{
    "name": "statical",
    "version": "1.0.0",
    "description": "Static site generator boilerplate",
    "main": "index.js",
    "scripts": {
        "dev": "node ./bin/dev.js",
        "scaffold": "node ./bin/scaffold.js"
    },
    "author": "Kevin Lint",
    "license": "ISC",
    "dependencies": {
        "async": "^1.2.1",
        "autoprefixer": "^5.2.0",
        "babel-core": "^5.5.8",
        "browser-sync": "^2.7.6",
        "colors": "^1.1.2",
        "cssnext": "^1.6.0",
        "csswring": "^3.0.5",
        "del": "^1.2.0",
        "front-matter": "^1.0.0",
        "fs": "0.0.2",
        "inquirer": "^0.8.5",
        "node-sass": "^3.1.2",
        "postcss": "^4.1.11",
        "postcss-custom-media": "^4.0.0",
        "swig": "^1.4.2",
        "terminal-menu": "^2.1.1"
    },
    "build": {
        "patterns": {
            "scripts": [],
            "styles": ["settings", "grid", "typography"]
        },
        "property": {
            "name": "kevinlint.com",
            "scripts": ["main"],
            "styles": ["main"]
        },
        "pages": ["index", "about"],
        "parts": ["menu", "head", "footer-scripts", "header"],
        "pieces": ["logo"]
    }
}